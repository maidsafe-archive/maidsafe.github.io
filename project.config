[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/David Irvine (dirvine)
[access "refs/for/refs/*"]
	read = group user/David Irvine (dirvine)
	push = group user/David Irvine (dirvine)
	label-Code-Review = -2..+2 group user/David Irvine (dirvine)
	label-Verified = -1..+1 group user/David Irvine (dirvine)
	rebase = group user/David Irvine (dirvine)
	removeReviewer = group user/David Irvine (dirvine)
	submit = group user/David Irvine (dirvine)
[access "refs/heads/*"]
	read = group user/David Irvine (dirvine)
	create = group user/David Irvine (dirvine)
	push = +force group user/David Irvine (dirvine)
	pushMerge = group user/David Irvine (dirvine)
[access "refs/tags/*"]
	push = +force group user/David Irvine (dirvine)
	pushTag = group user/David Irvine (dirvine)
	pushSignedTag = group user/David Irvine (dirvine)
